  },
  "dependencies": {
    "core-js": "^3.6.5",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "next": "9.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.3",
    "styled-jsx": "^3.2.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/node": "^13.11.1"
  }
}
