{
  "name": "cloud_sql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server/bootstrap.js",
    "start:k": "NODE_ENV=production node server/bootstrap.js",
    "build": "next build",
    "start": "next start",
    "serve:dev": "NODE_PATH=app NODE_ENV=dev nodemon app/bootstrap.js",
